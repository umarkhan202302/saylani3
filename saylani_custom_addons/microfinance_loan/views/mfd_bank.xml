<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record id="view_mfd_bank" model="ir.ui.view">
            <field name="name">view.mfd.bank</field>
            <field name="model">mfd.bank</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                    <field name="is_active" widget="boolean_toggle"/>
                </tree>
            </field>
        </record>

        <record id="action_mfd_bank" model="ir.actions.act_window">
            <field name="name">Bank</field>
            <field name="res_model">mfd.bank</field>
            <field name="view_mode">tree</field>
        </record>

    </data>
</odoo>